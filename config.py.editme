#!/usr/bin/python
# -*- coding: utf-8 -*-
# Description: Configuration file

################################################################
# BTG CONFIGURATION PARAMETERS
################################################################

debug = False
display_motd = True
offline = False # Force to use offline modules

# Specify modules folder
modules_folder = "./modules/"

log_found_file = "./log.txt"
log_search_file = "./search.txt"

# User-Agent for http requests
user_agent = {
    'User-Agent': "Mozilla/5.0 (X11; Linux x86_64; rv:45.0) Gecko/20100101 Firefox/45.0",
}
proxy_host = {
    "http": "",
    "https": ""
}
requests_timeout = 5 # seconds

temporary_cache_path = "./dumps/"
temporary_cache_update = 120 # Update cache every n minutes

# SQlite3 path (for specific modules using requests_cache)
sqlite_path = "/tmp/"

# Specify maximum child process for BTG
max_process = 4


################################################################
# Dshield - domains
################################################################
