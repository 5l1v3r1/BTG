################################################################
# MISP - MD5/SHA1/SHA256/URL/IPv4/domain/IPv6/SHA512
################################################################
misp_enabled = False
misp_url = ""
misp_key = ""
misp_verifycert = True
#this settings allows to display tags from the tlp namespace
misp_tag_display = "tlp"  

################################################################
# TOR exits nodes - 
################################################################
tor_enabled = False

################################################################
# Openphish - URL
################################################################
openphish_enabled = False

################################################################
